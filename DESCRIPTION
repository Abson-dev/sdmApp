Package: sdmApp
Title: A User-Friendly Application for Species Distribution Modeling
Version: 0.0.2
Authors@R: c(
    person("Aboubacar", "HEMA", , "aboubacarhema94@gmail.com", role = c("aut", "cre")),
    person("Babacar", "NDAO", , "babacar.ndao@cse.sn", role = "aut"),
